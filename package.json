{
  "name": "mkn",
  "description": "Make React Project w/ Next.js and Typescript",
  "version": "0.2.0",
  "license": "MIT",
  "author": "Jason Feng <solidzoro@live.com>",
  "private": true,
  "scripts": {
    "-------------------------------- LINT ----": "----",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "fix": "yarn lint --fix",
    "tsc-check": "tsc -v && tsc --noEmit",
    "format": "prettier --write src/**/*.ts{,x}",
    "prepare": "husky install",
    "-------------------------------- TOOL ----": "----",
    "versionup": "standard-version -r patch",
    "versionup:minor": "standard-version -r minor",
    "versionup:major": "standard-version -r major",
    "versionup:init": "standard-version --first-release",
    "-------------------------------- DEV  ----": "----",
    "dev": "next dev",
    "start": "next start",
    "serve": "yarn build && yarn start",
    "-------------------------------- PROD ----": "----",
    "build": "next build"
  },
  "dependencies": {
    "antd": "^4.16.6",
    "autoprefixer": "^10.2.6",
    "axios": "^0.21.0",
    "babel-plugin-add-react-displayname": "^0.0.5",
    "classnames": "^2.3.1",
    "css-loader": "^5.2.6",
    "next": "11.0.1",
    "next-plugin-antd-less": "^1.3.0",
    "normalize.css": "^8.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet-async": "^1.0.9",
    "react-icons": "^4.1.0",
    "react-use": "^17.2.4"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^12.1.4",
    "@types/classnames": "^2.3.1",
    "@types/eslint": "^7.2.13",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/node": "^15.14.0",
    "@types/prettier": "^2.3.1",
    "@types/react": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "commitlint": "^12.1.4",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "typescript": "^4.3.5"
  }
}
